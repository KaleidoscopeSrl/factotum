{
    "name": "kaleidoscope/dev-factotum",
    "description": "Laravel-based CMS baked by Kaleidoscope Srl",
    "keywords": ["cms", "laravel"],
    "license": "MIT",
    "language": "php",
    "type": "cms-project",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.8",
        "intervention/image": "^2.4.2",
        "froala/wysiwyg-editor-php-sdk" : "1.0.1",
        "barryvdh/laravel-debugbar": "^3.1.5",
        "laravel/passport": "7.4.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Kaleidoscope\\Factotum\\":                   "src",
            "Kaleidoscope\\Factotum\\Library\\":          "src/app/Library",
            "Kaleidoscope\\Factotum\\Helpers\\":          "src/app/Helpers",
            "Kaleidoscope\\Factotum\\Http\\Middleware\\": "src/app/Http/Middleware",
            "Kaleidoscope\\Factotum\\Mail\\":             "src/app/Mail"
        },
        "files": [
            "src/app/Library/ContentSearch.php",
            "src/app/Library/ContentListParser.php",
            "src/app/Library/Utility.php"
        ]
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Filippo Matteo Riggio",
            "email": "filippo@kaleidoscope.it"
        }
    ]
}
