{
    "name": "kaleidoscope/dev-factotum",
    "description": "Laravel-based CMS baked by Kaleidoscope Srl",
    "keywords": ["cms", "laravel"],
    "license": "MIT",
    "language": "php",
    "type": "cms-project",
    "authors": [
      {
        "name": "Filippo Matteo Riggio",
        "email": "filippo@kaleidoscope.it"
      }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.8",
        "intervention/image": "^2.4.2",
        "barryvdh/laravel-debugbar": "^3.1.5",
        "laravel/passport": "7.4.*",
        "spatie/laravel-sitemap": "^5.7",
        "stripe/stripe-php": "^7.37",
        "laravel/ui": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database",
            "src",
            "src/app",
            "src/app/Console",
            "src/app/Console/Commands",
            "src/app/Observers"
        ],
        "psr-4": {
            "Kaleidoscope\\Factotum\\":                       "src",
            "Kaleidoscope\\Factotum\\Console\\":              "src/app/Console",
            "Kaleidoscope\\Factotum\\Console\\Commands\\":    "src/app/Console/Commands",
            "Kaleidoscope\\Factotum\\Library\\":              "src/app/Library",
            "Kaleidoscope\\Factotum\\Helpers\\":              "src/app/Helpers",
            "Kaleidoscope\\Factotum\\Http\\Middleware\\":     "src/app/Http/Middleware",
            "Kaleidoscope\\Factotum\\Http\\Requests\\":       "src/app/Http/Requests",
            "Kaleidoscope\\Factotum\\Http\\Policies\\":       "src/app/Http/Policies",
            "Kaleidoscope\\Factotum\\Observers\\":            "src/app/Observers",
            "Kaleidoscope\\Factotum\\Mail\\":                 "src/app/Mail",
            "Kaleidoscope\\Factotum\\Filters\\":              "src/app/Filters",
        },
        "files": [
            "src/app/Library/ContentSearch.php",
            "src/app/Library/ContentListParser.php",
            "src/app/Library/Utility.php"
        ]
    },
    "minimum-stability": "dev",
}
