{
    "name": "kaleidoscope/factotum",
    "description": "Laravel-based CMS baked by Kaleidoscope Srl",
    "keywords": ["cms", "laravel"],
    "license": "MIT",
    "type": "cms-project",
    "require": {
        "php": ">=5.6.4",
        "laravel/laravel": "^5.4",
        "intervention/image": "^2.3",
        "froala/wysiwyg-editor-php-sdk" : "1.0.1",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Kaleidoscope\\Factotum\\Library\\": "packages/Kaleidoscope/Factotum/src/app/Library",
            "Kaleidoscope\\Factotum\\Helpers\\": "packages/Kaleidoscope/Factotum/src/app/Helpers",
            "Kaleidoscope\\Factotum\\Http\\Middleware\\": "packages/Kaleidoscope/Factotum/src/app/Http/Middleware"
        },
        "files": [
            "Kaleidoscope/Factotum/Library/ContentSearch.php",
            "Kaleidoscope/Factotum/Library/ContentListParser.php",
            "Kaleidoscope/Factotum/Library/Utility.php",
            "vendor/froala/wysiwyg-editor-php-sdk/lib/FroalaEditor.php"
        ]
    },
    "authors": [
        {
            "name": "Filippo Matteo Riggio",
            "email": "filippo@kaleidoscope.it"
        }
    ]
}